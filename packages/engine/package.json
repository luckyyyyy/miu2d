{
  "name": "@miu2d/engine",
  "version": "0.0.0",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./audio": "./src/audio/index.ts",
    "./character": "./src/character/index.ts",
    "./character/*": "./src/character/*.ts",
    "./character/level": "./src/character/level/index.ts",
    "./character/level/*": "./src/character/level/*.ts",
    "./core": "./src/core/index.ts",
    "./core/*": "./src/core/*.ts",
    "./renderer": "./src/renderer/index.ts",
    "./renderer/*": "./src/renderer/*.ts",
    "./runtime": "./src/runtime/index.ts",
    "./runtime/*": "./src/runtime/*.ts",
    "./gui": "./src/gui/index.ts",
    "./gui/*": "./src/gui/*.ts",
    "./gui/contract": "./src/gui/contract.ts",
    "./magic": "./src/magic/index.ts",
    "./magic/*": "./src/magic/*.ts",
    "./map": "./src/map/index.ts",
    "./map/*": "./src/map/*.ts",
    "./npc": "./src/npc/index.ts",
    "./obj/*": "./src/obj/*.ts",
    "./player": "./src/player/index.ts",
    "./player/*": "./src/player/*.ts",
    "./player/goods": "./src/player/goods/index.ts",
    "./player/goods/*": "./src/player/goods/*.ts",
    "./player/magic/*": "./src/player/magic/*.ts",
    "./resource": "./src/resource/index.ts",
    "./resource/*": "./src/resource/*.ts",
    "./script": "./src/script/index.ts",
    "./sprite": "./src/sprite/index.ts",
    "./sprite/*": "./src/sprite/*.ts",
    "./utils": "./src/utils/index.ts",
    "./weather": "./src/weather/index.ts",
    "./wasm": "./src/wasm/index.ts",
    "./wasm/*": "./src/wasm/*.ts"
  },
  "scripts": {
    "tsc": "tsc -b",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check --write ."
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@types/bun": "^1.3.8",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.2.5",
    "vitest": "^4.0.18"
  },
  "optionalDependencies": {
    "@miu2d/engine-wasm": "workspace:*"
  }
}
